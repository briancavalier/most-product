{
  "name": "most-product",
  "version": "1.1.1",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "author": "Brian Cavalier <brian@briancavalier.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.78.0",
    "flow-copy-source": "^2.0.2",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "@most/core": "^1.3.2",
    "@most/prelude": "^1.7.2",
    "@most/types": "^1.0.1"
  },
  "scripts": {
    "build": "rollup -c && flow-copy-source -i '*.test.js' src dist",
    "test:lint": "standard --fix --verbose 'src/**/*.js' | snazzy",
    "test:typecheck": "flow check",
    "test:unit": "nyc mocha -r babel-register src/**/*.test.js",
    "test": "run-p test:lint test:typecheck test:unit",
    "prepare": "run-s build"
  },
  "babel": {
    "presets": [
      "env",
      "flow"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugings": [
      "flowtype"
    ]
  }
}
